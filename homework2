import java.util.*;
public class StringSerach {
public static void main(String[] args)
{
  int digitCount=0,capitalCount=0;
	String str=" ",ahmed=" ";
	Scanner input=new Scanner(System.in);
	str=input.nextLine();
	int index=str.indexOf("Ahmad");
	if (index>=0)
		System.out.println("Hello Ahmÿ¥d");
	int charnum=str.length();
	char[] string=str.toCharArray();
	for (int i=0;i<string.length;i++)
	{
		if (Character.isDigit(string[i]))
			digitCount++;
		if(Character.isUpperCase(string[i]))
			capitalCount++;
		
	}
		System.out.printf("number of character : %d\nnumber of digits : %d\nnumber of Capital character : %d\n",charnum,digitCount,capitalCount);
		
}
}
